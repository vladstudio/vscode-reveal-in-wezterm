{
  "name": "vscode-reveal-in-wezterm",
  "displayName": "Reveal in WezTerm",
  "description": "Reveal files and project in WezTerm terminal",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "revealInWezterm.revealFile",
        "title": "Reveal in WezTerm"
      },
      {
        "command": "revealInWezterm.revealProject",
        "title": "Reveal Project in WezTerm"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "revealInWezterm.revealFile",
          "when": "explorerResourceIsFolder || explorerResourceIsFile"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "dev": "npm run compile"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "open": "^8.4.0"
  }
}